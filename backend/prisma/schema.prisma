generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
    id Int @id @default(autoincrement())
    name String
    layers LayerType[]
}
model LayerType {
    id Int @id @default(autoincrement())
    label String
    value String
    projectId Int
    project Project @relation(fields: [projectId], references: [id])
}